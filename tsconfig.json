{
    "compilerOptions": {
      "outDir": "./build/",
      "noImplicitAny": false,
      "sourceMap": true,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "baseUrl": "src",
      "paths": {
        "modules/*": ["modules/*"],
        "reducers/*": ["reducers/*"],
        "store/*": ["store/*"],
        "routes/*": ["routes/*"],
        }
    }
  }